/*我的订单*/
.my_order {
  padding: 0px 32px;
  width: 100%;
  height: 45px;
  margin-top: 45px;
  background: #FFFFFF;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.my_order .my_order_item {
  display: flex;
  align-items: center;
  line-height: 44px;
  cursor: pointer;
  border-bottom: 2px solid #FFFFFF;
  font-size: 1.4rem;
}
.my_order .my_order_item.cl_active {
  color: #ff7403;
  border-bottom: 2px solid red;
}
.my_order_con {
  padding: 0px 10px;
  width: 100%;
  margin-top: 10px;
  background: #ffffff;
}
.my_order_con .con_item_t {
  width: 100%;
  height: 90px;
  background-image: linear-gradient(#fef9f3, #fef9f3), linear-gradient(#fef9f3, #fef9f3);
  background-blend-mode: normal, 
        normal;
  border-top-left-radius: 12px;
  border-top-right-radius: 12px;
  border-bottom-left-radius: 12px;
  border-bottom-right-radius: 12px;
  border: solid 1px #f7e7d0;
  border-bottom: dashed 1px #f7e7d0;
}
.my_order_con .con_item_b {
  width: 100%;
  height: 30px;
  background-image: linear-gradient(#fef9f3, #fef9f3), linear-gradient(#fef9f3, #fef9f3);
  background-blend-mode: normal, 
        normal;
  border-top-left-radius: 12px;
  border-top-right-radius: 12px;
  border-bottom-left-radius: 12px;
  border-bottom-right-radius: 12px;
  border: solid 1px #f7e7d0;
  border-top: none;
}
